{% extends "base.twig" %}

{# ABOUT PAGE #}

{% block content %}
	<div class="content-wrapper">
		<article class="post-type-{{post.post_type}}" id="post-{{post.ID}}">
			<section class="article-content">
				<h1 class="article-h1">{{post.title}}</h1>
				<div class="article-body">
					{{post.content}}
					
					<div>THIS IS THE ABOUT PAGE!!!</div>

				</div>
			</section>
		</article>
	</div><!-- /content-wrapper -->
{% endblock %}